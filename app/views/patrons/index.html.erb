<%- case when @work -%>
  <%= render :partial => 'index_work', :locals => {:work => @work} -%>
<%- when @expression -%>
  <%= render :partial => 'index_expression', :locals => {:expression => @expression} -%>
<%- when @manifestation -%>
  <%= render :partial => 'index_manifestation', :locals => {:manifestation => @manifestation} -%>
<%- when @patron -%>
  <%= render :partial => 'index_patron', :locals => {:patron => @patron} -%>
<%- when @patron_merge_list -%>
  <%= render :partial => 'index_patron_merge_list', :locals => {:patron_merge_list => @patron_merge_list} -%>
<%- else -%>
  <%= render :partial => 'index' -%>
<%- end -%>
